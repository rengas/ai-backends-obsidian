    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "^1.8.7",
    "tslib": "2.4.0",
    "typescript": "^4.7.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.5",
    "js-yaml": "^4.1.0"
  }
