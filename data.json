{
  "apiUrl": "http://localhost:3000",
  "apiKey": "",
  "configFilePath": "ai-config/config.yaml",
  "defaultProvider": "ollama",
  "defaultModel": "gemma3:4b",
  "defaultTemperature": 0.3,
  "defaultStream": true,
  "summarize": {
    "provider": "ollama",
    "model": "gemma3:4b",
    "temperature": 0.3,
    "stream": true,
    "maxLength": 100
  },
  "keywords": {
    "provider": "ollama",
    "model": "mistrallite:latest",
    "temperature": 0.3,
    "stream": false,
    "maxKeywords": 500
  },
  "translate": {
    "provider": "ollama",
    "model": "gemma3:4b",
    "temperature": 0.1,
    "stream": true,
    "defaultTargetLanguage": "en"
  },
  "rewrite": {
    "provider": "ollama",
    "model": "gemma3:4b",
    "temperature": 0.3,
    "stream": true
  },
  "compose": {
    "provider": "ollama",
    "model": "gemma3:4b",
    "temperature": 0.3,
    "stream": true,
    "maxLength": 50
  }
}